{
   "action": {
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "https://www.linkedin.com/*" ]
   } ],
   "description": "Automate LinkedIn connection requests efficiently and professionally",
   "host_permissions": [ "https://www.linkedin.com/*" ],
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 3,
    "name": "Linkedin Connect, Message and Follow",
   "permissions": [ "scripting", "activeTab", "alarms", "storage", "downloads" ],
   "version": "1.0"
}
